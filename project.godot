; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

git_describe/describe_setting_path="application/config/version"

[application]

config/name="Haunting Hijinx"
run/main_scene="res://src/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="haunting_hijinx"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/use_filter=false
boot_splash/image="res://assets/made_with_godot.png"

[autoload]

GlobalSounds="res://src/autoload/global_sounds/global_sounds.tscn"
InterfaceSounds="res://src/autoload/interface_sounds.gd"
PeerData="*res://src/autoload/peer_data/peer_data.tscn"
MenuBackground="res://src/autoload/menu_background/menu_background.tscn"
SceneChanger="*res://src/autoload/scene_changer/scene_changer.tscn"
FullscreenToggle="res://src/autoload/fullscreen_toggle.gd"
MouseHider="res://src/autoload/mouse_hider.gd"
WindowScaler="*res://src/autoload/window_scaler.gd"
PromptManager="*res://addons/input_prompts/input_prompt_manager.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/vsync/vsync_mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/gdUnit4/plugin.cfg", "res://addons/git_describe/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg", "res://addons/input_prompts/plugin.cfg", "res://addons/licenses/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gdunit4]

ui/toolbar/run_overall=true
templates/testsuite/GDScript="class_name ${suite_class_name}
extends GdUnitTestSuite
"

[gui]

theme/custom="res://src/theme/theme.tres"

[importer_defaults]

font_data_dynamic={
"Compress": null,
"Fallbacks": null,
"Rendering": null,
"allow_system_fallback": true,
"antialiasing": 0,
"compress": true,
"disable_embedded_bitmaps": true,
"fallbacks": [],
